
<%= simple_form_for(@qualification) do |f| %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description,input_html:{rows:6} %>
    <%= f.association :user ,as: :hidden%>
  </div>

  <div class="form-actions">
    <%= f.button :submit , :class=>"btn btn-success"%>
    <%= link_to "Back", qualifications_path, :class=>"btn btn-info"%>
  </div>
<% end %>
